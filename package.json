{
  "name": "learning-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "check-types": "tsc --pretty --noEmit",
    "check-lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "check-format": "prettier --check .",
    "format": "prettier --write .",
    "test-all": "pnpm check-format && pnpm check-lint && pnpm check-types && pnpm build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@tanstack/react-router": "^1.15.19",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@tanstack/router-vite-plugin": "^1.15.18",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^9.0.10",
    "postcss": "^8.4.34",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
